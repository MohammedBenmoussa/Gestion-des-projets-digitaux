<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
<class name="com.yoprojet.projet.dao.entite.BesoinProjet" table="BESOINPROJET" >
	<id name="id" column="ID"  >
		<generator class="native"></generator>
	</id>
	<property name="description" column="DESCRIPTION"></property>
	<property name="idMetier" column="IDMETIER"></property>
	<property name="idClient" column="IDCLIENT"></property>
	<property name="nomComplet" column="NOMCOMPLET"></property>
	<property name="nomCourt" column="NOMCOURT"></property>
	<property name="budget" column="BUDGET"></property>
	<property name="coutEstime" column="COUTESTIME"></property>
	<property name="active" column="ACTIVE"></property>
	<property name="idChef" column="IDCHEF"></property>
</class>

</hibernate-mapping>