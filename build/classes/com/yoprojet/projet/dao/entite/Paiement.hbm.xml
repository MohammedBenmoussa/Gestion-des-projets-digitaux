<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.yoprojet.projet.dao.entite.Paiement" table="Paiement" >
	<id name="id" column="ID"  >
		<generator class="native"></generator>
	</id>
	<property name="tx" column="TX"></property>
	<property name="idClient" column="IDCLIENT"></property>
	<property name="idProjet" column="IDPROJET"></property>
	<property name="idChef" column="IDCHEF"></property>
	<property name="montant" column="MONTANT"></property>
	<property name="date" column="DATE"></property>
	</class>
</hibernate-mapping>